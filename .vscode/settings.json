{
    // Modify the com_port settings accordingly
    // 1) Windows: check the com_port using the device manager, find the one with "GDB SERVER"
    //      -> "com_port": "\\\\.\\COMx"
    // 2) Linux: use "List Epuck2 ports" task
    //      -> "com_port": "/dev/ttyACMx"
    // MacOS: use the command "ls /dev/cu.*" and use the lowest one
    //      -> "com_port": "/dev/cu.usbmodemEPUCKx"
    "com_port": "/dev/cu.usbmodemEPUCK1",

    // Add your SerialMonitor port, the same used for communicating with SD3 of F407
    // This port is used too for ESP32 programming but need switching by the programmer
    // Used by the "Tools: Update ESP32 firmware" task to automatize the job
    "serial_monitor": "/dev/cu.usbmodemEPUCK3"
}